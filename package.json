{
  "name": "fstruct-cli",
  "version": "0.0.1-alpha",
  "description": "Command line tool",
  "engines": {
    "node": ">=18.16.0"
  },
  "bin": {
    "fstruct": "./dist/index.js"
  },
  "directories": {
    "bin": "./dist"
  },
  "files": [
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --build",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "watch": "tsc --watch",
    "prewatch": "npm run clean",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm lint && npm prettier && npm test",
    "prettier": "prettier --write '**/*.{js,json,css,md}'",
    "lint": "eslint --fix ./src",
    "test": "ava",
    "test:watch": "npm test --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "fstruct",
    "commander",
    "commanderjs",
    "command-line-tool",
    "cli",
    "command-line",
    "command line",
    "cl",
    "terminal",
    "node",
    "nodejs"
  ],
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "micromatch": "^4.0.5",
    "rollup-plugin-add-shebang": "^0.3.1",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/fs-extra": "^11.0.2",
    "@types/micromatch": "^4.0.3",
    "@types/mock-fs": "^4.13.2",
    "@types/node": "^18.16.0",
    "ava": "^5.3.1",
    "eslint": "^4.19.1",
    "husky": "^1.0.0-rc.7",
    "lint-staged": "^7.1.2",
    "mock-fs": "^5.2.0",
    "prettier": "^1.12.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "ts-loader": "^9.5.0",
    "tslib": "^2.6.2",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  }
}
